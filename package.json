{
  "name": "vscode_starter",
  "version": "1.0.0",
  "description": "",
  "main": "dist/extension.js",
  "engines": {
    "vscode": "^1.74.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "vite",
    "build": "npm run setup && vite build",
    "preview": "vite preview",
    "start": "npm run setup && vite build --watch",
    "open": "code --extensionDevelopmentPath=${PWD}",
    "setup": "node dynamic-contributes.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.3",
    "@types/node": "^20.4.5",
    "@types/vscode": "^1.80.0",
    "@vitejs/plugin-react": "^4.0.3",
    "dotenv": "^16.3.1",
    "glob": "^10.3.3",
    "vite": "^4.4.7"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "svelte": "^4.1.1"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "container",
          "title": "Extension Starter",
          "icon": "public/bell.svg"
        }
      ]
    },
    "views": {
      "container": [
        {
          "id": "box",
          "name": "box",
          "type": "webview",
          "initialSize": 4
        }
      ]
    },
    "commands": [],
    "configuration": {
      "type": "object",
      "title": "MagicBell Ping",
      "properties": {
        "ping.githubAccessToken": {
          "type": "string",
          "description": "Your github access token. Refer to Ping's VSCode Extension page for a step-by-step guide on how to create your token."
        }
      }
    },
    "keybindings": []
  }
}